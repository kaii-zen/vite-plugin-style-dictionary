{
  "name": "vite-plugin-style-dictionary",
  "description": "Vite plugin that runs Style Dictionary using Vite's module graph.",
  "version": "0.0.1",
  "type": "module",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/kaii-zen/vite-plugin-style-dictionary.git"
  },
  "bugs": {
    "url": "https://github.com/kaii-zen/vite-plugin-style-dictionary/issues"
  },
  "homepage": "https://github.com/kaii-zen/vite-plugin-style-dictionary#readme",
  "keywords": [
    "vite",
    "vite-plugin",
    "style-dictionary",
    "design-tokens",
    "tokens"
  ],
  "release": {
    "branches": [
      "main"
    ],
    "plugins": [
      [
        "@semantic-release/commit-analyzer",
        {
          "preset": "conventionalcommits"
        }
      ],
      [
        "@semantic-release/release-notes-generator",
        {
          "preset": "conventionalcommits"
        }
      ],
      [
        "@semantic-release/exec",
        {
          "prepareCmd": "npm version ${nextRelease.version} --no-git-tag-version",
          "publishCmd": "npm publish --provenance"
        }
      ],
      [
        "@semantic-release/git",
        {
          "assets": [
            "package.json",
            "package-lock.json"
          ],
          "message": "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
        }
      ],
      "@semantic-release/github"
    ]
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format esm --dts --clean",
    "dev": "tsup src/index.ts --format esm --dts --watch",
    "test": "vitest run",
    "typecheck": "tsc -p tsconfig.json --noEmit"
  },
  "dependencies": {
    "lodash-es": "^4.17.21",
    "style-dictionary": "^5.1.1"
  },
  "peerDependencies": {
    "vite": "^7.0.0"
  },
  "devDependencies": {
    "@semantic-release/commit-analyzer": "^12.0.0",
    "@semantic-release/exec": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^10.0.0",
    "@semantic-release/release-notes-generator": "^13.0.0",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^20.19.3",
    "conventional-changelog-conventionalcommits": "^7.0.2",
    "semantic-release": "^25.0.2",
    "tsup": "^8.0.1",
    "typescript": "^5.3.0",
    "vite": "^7.0.4",
    "vitest": "^3.2.4"
  }
}
